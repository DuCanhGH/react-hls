{
  "name": "@ducanh2912/react-hls-player",
  "author": "Christopher Short, DuCanhGH",
  "description": "A simple and easy to use react component for playing an hls live stream",
  "version": "3.0.11",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14.6"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint src test",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "format": "prettier --write ."
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DuCanhGH/react-hls.git"
  },
  "keywords": [
    "hls",
    "rtmp",
    "react",
    "component"
  ],
  "bugs": {
    "url": "https://github.com/DuCanhGH/react-hls/issues"
  },
  "homepage": "https://github.com/DuCanhGH/react-hls#README",
  "module": "dist/react-hls-player.esm.js",
  "size-limit": [
    {
      "path": "dist/react-hls-player.cjs.production.min.js",
      "limit": "244 KB"
    },
    {
      "path": "dist/react-hls-player.esm.js",
      "limit": "244 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-big-lib": "8.0.0",
    "@size-limit/webpack": "8.0.0",
    "@size-limit/webpack-why": "8.0.0",
    "@testing-library/react": "13.3.0",
    "@types/jest": "28.1.6",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "husky": "8.0.1",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "size-limit": "8.0.0",
    "tsdx": "0.14.1",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "webpack": "5.74.0"
  },
  "dependencies": {
    "hls.js": "1.2.0"
  }
}
